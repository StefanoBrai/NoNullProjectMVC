@model IEnumerable<WebApplication1.Models.Request>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BeginningDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndingDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Destination)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Proj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Skill)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BeginningDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndingDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comp.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Destination.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Proj.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Skill.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ReqId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ReqId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ReqId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
